{
  "expo": {
    "name": "Expo Turtle CLI",
    "slug": "expo-turtle-cli",
    "privacy": "unlisted",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "description": "Expo Turtle CLI project is an experiment to build Expo projects locally"
  }
}

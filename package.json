{
  "name": "expo-turtle-cli",
  "version": "1.0.0",
  "description": "Expo Turtle CLI project is an experiment to build Expo projects locally",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "export": "expo export --public-url https://develon.github.io/RN-Expo-Turtle-CLI/dist && rm -rf ~/.turtle/workingdir && open https://develon.github.io/RN-Expo-Turtle-CLI/dist/android-index.json",
    "build:apk": "turtle build:android --public-url https://develon.github.io/RN-Expo-Turtle-CLI/dist/android-index.json -t apk -d $(pwd)/build/android/",
    "build:aab": "turtle build:android --public-url https://develon.github.io/RN-Expo-Turtle-CLI/dist/android-index.json -d $(pwd)/build/android/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/develon/RN-Expo-Turtle-CLI"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.5.1",
    "@react-navigation/compat": "^5.1.25",
    "@react-navigation/drawer": "5.12.4",
    "@react-navigation/native": "5.9.3",
    "@react-navigation/stack": "5.14.3",
    "expo": "^40.0.0",
    "expo-app-loading": "^1.0.1",
    "expo-asset": "~8.2.1",
    "expo-font": "~8.4.0",
    "galio-framework": "^0.7.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-modal-dropdown": "git+https://github.com/siemiatj/react-native-modal-dropdown.git",
    "react-native-reanimated": "~1.13.0",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "~2.15.2",
    "react-native-svg": "12.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-preset-expo": "8.3.0"
  },
  "author": "Santhosh Umapathi",
  "homepage": "https://develon.github.io/RN-Expo-Turtle-CLI/dist"
}
